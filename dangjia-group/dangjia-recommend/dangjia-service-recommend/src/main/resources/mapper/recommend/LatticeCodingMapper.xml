<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dangjia.acg.mapper.recommend.ILatticeCodingMapper">

    <!-- 查询编号名称列表 - 根据编码值列表 -->
    <select id="queryCodingNameList" resultType="java.lang.String">
        SELECT coding_name
        FROM dj_lattice_coding
        WHERE coding_value IN
        <foreach collection="valueList" index="index" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>

</mapper>