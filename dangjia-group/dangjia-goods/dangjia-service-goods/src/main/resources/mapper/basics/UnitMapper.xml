<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dangjia.acg.mapper.basics.IUnitMapper">
    <sql id="Base_Column_List">
    id, name, create_date, modify_date
  </sql>

    <select id="getUnit" resultType="com.dangjia.acg.modle.brand.Unit">
        SELECT
           id, name, create_date as createDate, modify_date as modifyDate
        from dj_basics_unit
        ORDER BY create_date desc
    </select>

</mapper>